  "author": "Thinh Dinh",
  "params": [
    {
      "name": "data",
      "displayName": "Primary Timeline data (JSON string)",
      "type": "string"
    },
    {
      "name": "overlayDataJson",
      "displayName": "Overlay Lines Data (JSON string)",
      "type": "string"
    },
    {
      "name": "intervalName",
      "displayName": "Interval Name",
      "type": "string",
      "defaultValue": "Period"
    },
    {
      "name": "type",
